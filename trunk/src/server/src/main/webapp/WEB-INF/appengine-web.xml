<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>sms-test</application>
	<version>1</version>
	<sessions-enabled>true</sessions-enabled>
	<ssl-enabled>true</ssl-enabled>
	<static-files>
		<include path="/**.*" />
	</static-files>
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/classes/log4j.xml" />
		<property name="org.apache.commons.logging.Log" value="org.apache.commons.logging.impl.Log4JLogger"/>
	</system-properties>
</appengine-web-app>
